# CONFIG_PACKAGE_luci-app-ddnsto is not set
CONFIG_PACKAGE_luci-app-store=y
CONFIG_PACKAGE_app-store-ui=y
CONFIG_PACKAGE_nano=y
CONFIG_PACKAGE_frpc=y
CONFIG_PACKAGE_luci-app-frpc=y
CONFIG_PACKAGE_luci-i18n-frpc-zh-cn=y


CONFIG_PACKAGE_wireguard=yes
CONFIG_PACKAGE_wireguard-tools=yes
CONFIG_PACKAGE_kmod-wireguard=yes
CONFIG_PACKAGE_luci-app-wireguard=yes

# Extra Packages
CONFIG_PACKAGE_autocore-x86=y

### Ethernet Support I225
CONFIG_PACKAGE_kmod-igc=y

CONFIG_PACKAGE_NTFS-3G_HAS_PROBE=y
CONFIG_PACKAGE_ntfs-3g=y
CONFIG_PACKAGE_automount=y

CONFIG_PACKAGE_iperf=y

CONFIG_PACKAGE_gzip=y
CONFIG_PACKAGE_screen=y
CONFIG_PACKAGE_tree=y
CONFIG_PACKAGE_usbutils=y

CONFIG_PACKAGE_luci-app-turboacc=y
CONFIG_PACKAGE_TURBOACC_INCLUDE_BBR_CCA=y
CONFIG_PACKAGE_TURBOACC_INCLUDE_DNSFORWARDER=y
CONFIG_PACKAGE_TURBOACC_INCLUDE_DNSPROXY=y
CONFIG_PACKAGE_TURBOACC_INCLUDE_OFFLOADING=y
CONFIG_PACKAGE_TURBOACC_INCLUDE_SHORTCUT_FE=n
CONFIG_PACKAGE_luci-app-diskman=y
CONFIG_PACKAGE_luci-app-diskman_INCLUDE_btrfs_progs=y
CONFIG_PACKAGE_luci-app-diskman_INCLUDE_lsblk=y
CONFIG_PACKAGE_luci-app-diskman_INCLUDE_mdadm=y
CONFIG_PACKAGE_luci-app-diskman_INCLUDE_kmod_md_raid456=y
CONFIG_PACKAGE_luci-app-diskman_INCLUDE_kmod_md_linear=y
#
# Image Options
#
CONFIG_TARGET_KERNEL_PARTSIZE=64
CONFIG_TARGET_ROOTFS_PARTSIZE=960
CONFIG_TARGET_ROOTFS_PARTNAME=""
# end of Target Images





# 5. Protocols
#
# CONFIG_PACKAGE_luci-proto-3g is not set
CONFIG_PACKAGE_luci-proto-bonding=y
# CONFIG_PACKAGE_luci-proto-ipip is not set
# CONFIG_PACKAGE_luci-proto-ipv6 is not set
CONFIG_PACKAGE_luci-proto-openconnect=yes
CONFIG_PACKAGE_luci-proto-ppp=y
# CONFIG_PACKAGE_luci-proto-qmi is not set
# CONFIG_PACKAGE_luci-proto-relay is not set
# CONFIG_PACKAGE_luci-proto-vpnc is not set
CONFIG_PACKAGE_luci-proto-wireguard=yes
# end of 5. Protocols

